@model IEnumerable<AplicatieCamine.Models.Administratori>

@{
    ViewData["Title"] = "Index";
}

<h1>Administratori</h1>

<p>
    <a asp-action="Create">Add data</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.IdAdmin)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Nume)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Adresa)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NrTelefon)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Email)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdCaminNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.IdAdmin)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Nume)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Adresa)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NrTelefon)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdCaminNavigation.IdCamin)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdAdmin">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdAdmin">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdAdmin">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
